  <xsd:element name="person">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="name" type="xsd:integer"/>
        <xsd:element name="age" type="xsd:integer"/>

      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>
